    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    
    <bean id="emp1" class="employee.Employee_details">
    
    <property name="empid" value="012"></property>
    <property name="empname" value="Pavi"></property>
